{
  "name": "fe-console",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "",
  "sourceRoot": "apps/fe-console/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/fe-console",
        "browser": "apps/fe-console/src/main.ts",
        "tsConfig": "apps/fe-console/tsconfig.app.json",
        "inlineStyleLanguage": "css",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/fe-console/public"
          }
        ],
        "styles": [
          "apps/fe-console/src/styles.css",
          "overlayscrollbars/overlayscrollbars.css",
          "apps/fe-console/src/material.scss"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "fe-console:build:production"
        },
        "development": {
          "buildTarget": "fe-console:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4100,
        "ssl": true,
        "sslKey": ".ssl/localhost+1-key.pem",
        "sslCert": ".ssl/localhost+1.pem"
      }
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "fe-console:build"
      }
    },
    "test": {
      "executor": "@angular/build:unit-test"
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "fe-console:build",
        "staticFilePath": "dist/apps/fe-console/browser",
        "spa": true
      }
    }
  }
}
